clear
clc
close all;

%% Initialization
f = @(x) exp(3*x).*sin(200.*x.^2)./(1+20.*x.^2);
j = 4:14;
n = 2.^j;

for i = 1:length(n)
    x = 0:1/n(i):1;
    fx = f(x);
    xx = 0:.001:1;
    fxx = f(xx);
    splinex = spline(x,fx,xx);
    
end